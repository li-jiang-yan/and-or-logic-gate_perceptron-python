\documentclass[tikz, border=10pt]{standalone}

\usetikzlibrary{calc}

\def\layersep{3cm}
\newcommand\nn[2]{
    % Input layer
    \node[neuron] (i1-#1) at (0, 4) {$A$};
    \node[neuron] (i2-#1) at (0, 3) {$B$};
    \node[neuron] (i3-#1) at (0, 2) {$1$};

    % Output node
    \node[neuron] (o-#1) at (\layersep,3) {#2};

    % Connect every node in the hidden layer with the output layer
    \foreach \source in {1,...,3}
        \path (i\source-#1) edge (o-#1);
}

\begin{document}
\begin{tikzpicture}[
    scale=1.2,
    shorten >=1pt,->,draw=black!70, node distance=\layersep,
    neuron/.style={circle,fill=black!25,minimum size=20,inner sep=0},
    edge/.style={midway, font=\tiny},
    weight/.style={
        edge, fill=white, inner sep=2pt
      }
  ]
  \nn{or}{$A+B$}

  \begin{scope}[xshift=6cm]
    \nn{and}{$AB$}
  \end{scope}

  % Draw weights for all or edges.
  \path (i1-or) -- (o-or) node[weight={1}{1}] {114.37};
  \path (i2-or) -- (o-or) node[weight={2}{1}] {114.39};
  \path (i3-or) -- (o-or) node[weight={3}{1}] {-100.90};

  % Draw weights for all and edges.
  \path (i1-and) -- (o-and) node[weight={1}{1}] {42.56};
  \path (i2-and) -- (o-and) node[weight={2}{1}] {101.94};
  \path (i3-and) -- (o-and) node[weight={3}{1}] {-126.92};
\end{tikzpicture}
\end{document}